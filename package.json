{
	"author": "Alphka",
	"name": "flash-upload-server",
	"version": "1.3.0",
	"description": "A website that allows to archive Quality documents",
	"main": "src/index.ts",
	"scripts": {
		"build": "node bin/build",
		"build:dev": "cross-env NODE_ENV=development node bin/build",
		"build:watch": "node bin/build -- --watch",
		"build:watch:dev": "cross-env NODE_ENV=development node bin/build -- --watch",
		"server": "node bin/start",
		"server:dev": "cross-env NODE_ENV=development node bin/start",
		"server:watch": "nodemon",
		"server:watch:dev": "cross-env NODE_ENV=development nodemon",
		"start": "npm-run-all build server",
		"start:dev": "npm-run-all build:dev server:dev",
		"watch": "npm-run-all --parallel build:watch:dev server:watch:dev",
		"watch:dev": "npm-run-all --parallel build:watch:dev server:watch:dev",
		"webpack": "cross-env NODE_OPTIONS=\"--no-warnings --loader ts-node/esm\" TS_NODE_PROJECT=\"test/webpack.tsconfig.json\" webpack --config test/webpack.config.ts --node-env production"
	},
	"optionalDependencies": {
		"cross-env": "^7.0.3",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"ts-node-dev": "^2.0.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.15",
		"@types/glob": "^8.0.0",
		"@types/node": "^18.11.18",
		"sass": "^1.57.1",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-watched-glob-entries-plugin": "2.2.4"
	},
	"dependencies": {
		"axios": "^1.2.2",
		"express": "^4.18.2",
		"focus-trap": "^7.2.0",
		"mime": "^3.0.0",
		"pug": "^3.0.2",
		"socket.io": "^4.5.4"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Alphka/Flash-Upload-Server.git"
	},
	"bugs": {
		"url": "https://github.com/Alphka/Flash-Upload-Server/issues"
	},
	"homepage": "https://github.com/Alphka/Flash-Upload-Server#readme",
	"engines": {
		"node": ">=15.4",
		"npm": ">=7"
	},
	"keywords": [
		"archive",
		"cloud",
		"documents",
		"quality",
		"management",
		"senai"
	]
}
