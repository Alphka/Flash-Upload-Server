{
	"extends": "../tsconfig",
	"compilerOptions": {
		"module": "NodeNext",
		"target": "ESNext",
		"moduleResolution": "NodeNext",
		"allowJs": true,
		"checkJs": true,
		"strict": false,
		"noImplicitAny": false,
		"noStrictGenericChecks": false,
		"strictNullChecks": false,
		"esModuleInterop": false,
		"allowSyntheticDefaultImports": true,
		"typeRoots": [
			"typings",
			"../typings",
			"node_modules/@types"
		]
	}
}
